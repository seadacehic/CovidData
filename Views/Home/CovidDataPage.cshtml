
@{
    ViewBag.Title = "CovidDataPage";
}


<h2>CovidDataPage</h2>

<!DOCTYPE html>
<html>
<head>
    <title>
        App
    </title>
    <link href="~/assets/css/styles.css" rel="stylesheet">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    
</head>

<body>

    <h1>Corona virus</h1>
    <div id="container">
        <table id="covid" class="table table-bordered table-hover table-success">
            <!-- showing data -->
        </table>
        <button onclick="exporttable('covid', 'sample_excel')">Export to excel</button>
        <button id="json">Export to json</button>
    </div>

    <script src="~/assets/js/script.js"></script>
    <script src="~/assets/js/scripttwo.js"></script>
    <script src="https://rawcdn.githack.com/FuriosoJack/TableHTMLExport/v2.0.0/src/tableHTMLExport.js"></script>

    <script>

        $("#json").on("click", function () {
            $("#covid").tableHTMLExport({
                type: 'json',
                filename: 'sample.json'
            });
        });
    </script>
</body>
</html>
